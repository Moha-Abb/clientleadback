{
  "kind": "collectionType",
  "collectionName": "proyectos",
  "info": {
    "singularName": "proyecto",
    "pluralName": "proyectos",
    "displayName": "project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "en proceso",
        "terminado"
      ]
    },
    "price": {
      "type": "integer"
    },
    "finishdate": {
      "type": "date"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "projects"
    },
    "tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::task.task",
      "mappedBy": "project"
    },
    "userAsigned": {
      "type": "email"
    }
  }
}
